{
	"openapi": "3.0.0",
	"components": {
		"examples": {},
		"headers": {},
		"parameters": {},
		"requestBodies": {},
		"responses": {},
		"schemas": {
			"UUID": {
				"type": "string",
				"format": "uuid",
				"description": "Stringified UUIDv4.\r\nSee [RFC 4112](https://tools.ietf.org/html/rfc4122)",
				"pattern": "[0-9A-Fa-f]{8}-[0-9A-Fa-f]{4}-4[0-9A-Fa-f]{3}-[89ABab][0-9A-Fa-f]{3}-[0-9A-Fa-f]{12}"
			},
			"BacklogCompleteOut": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UUID"
					},
					"createdAt": {
						"type": "string",
						"format": "date-time"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time"
					},
					"deletedAt": {
						"type": "string",
						"format": "date-time",
						"nullable": true
					},
					"branchId": {
						"allOf": [
							{
								"$ref": "#/components/schemas/UUID"
							}
						],
						"nullable": true
					}
				},
				"required": [
					"id",
					"createdAt",
					"updatedAt",
					"branchId"
				],
				"type": "object",
				"additionalProperties": false
			},
			"BranchCompleteOut": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UUID"
					},
					"createdAt": {
						"type": "string",
						"format": "date-time"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time"
					},
					"deletedAt": {
						"type": "string",
						"format": "date-time",
						"nullable": true
					},
					"restaurantId": {
						"allOf": [
							{
								"$ref": "#/components/schemas/UUID"
							}
						],
						"nullable": true
					},
					"iOpen": {
						"type": "boolean",
						"nullable": true
					},
					"status": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50
					},
					"rating": {
						"type": "number",
						"format": "double",
						"nullable": true,
						"minimum": 0,
						"maximum": 5
					},
					"showRating": {
						"type": "boolean",
						"nullable": true
					},
					"instagram": {
						"type": "string",
						"nullable": true,
						"minLength": 5,
						"maxLength": 50
					},
					"telegram": {
						"type": "string",
						"nullable": true,
						"minLength": 5,
						"maxLength": 50
					},
					"twitter": {
						"type": "string",
						"nullable": true,
						"minLength": 5,
						"maxLength": 50
					},
					"youtube": {
						"type": "string",
						"nullable": true,
						"minLength": 5,
						"maxLength": 50
					},
					"eitaa": {
						"type": "string",
						"nullable": true,
						"minLength": 5,
						"maxLength": 50
					},
					"backlog": {
						"allOf": [
							{
								"$ref": "#/components/schemas/BacklogCompleteOut"
							}
						],
						"nullable": true
					}
				},
				"required": [
					"id",
					"createdAt",
					"updatedAt",
					"restaurantId",
					"backlog"
				],
				"type": "object",
				"additionalProperties": false
			},
			"RestaurantCompleteOut": {
				"properties": {
					"id": {
						"$ref": "#/components/schemas/UUID"
					},
					"createdAt": {
						"type": "string",
						"format": "date-time"
					},
					"updatedAt": {
						"type": "string",
						"format": "date-time"
					},
					"deletedAt": {
						"type": "string",
						"format": "date-time",
						"nullable": true
					},
					"branch": {
						"allOf": [
							{
								"$ref": "#/components/schemas/BranchCompleteOut"
							}
						],
						"nullable": true
					}
				},
				"required": [
					"id",
					"createdAt",
					"updatedAt"
				],
				"type": "object",
				"additionalProperties": false
			},
			"URL": {
				"type": "string",
				"format": "uri",
				"description": "URL format.",
				"pattern": "^(https?:\\/\\/)?(www\\.)?[-a-zA-Z0-9@:%._\\+~#=]{1,256}\\.[a-zA-Z0-9()]{1,6}\\b([-a-zA-Z0-9()@:%_\\+.~#?&//=]*)$"
			},
			"RestaurantCompactIn": {
				"properties": {
					"name": {
						"type": "string",
						"minLength": 2,
						"maxLength": 50
					},
					"slang": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50,
						"pattern": "^[a-zA-Z0-9]*$"
					},
					"instagram": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50
					},
					"telegram": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50
					},
					"twitter": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50
					},
					"youtube": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50
					},
					"eitaa": {
						"type": "string",
						"nullable": true,
						"minLength": 2,
						"maxLength": 50
					},
					"avatarUrl": {
						"allOf": [
							{
								"$ref": "#/components/schemas/URL"
							}
						],
						"nullable": true
					},
					"coverUrl": {
						"allOf": [
							{
								"$ref": "#/components/schemas/URL"
							}
						],
						"nullable": true
					},
					"logoUrl": {
						"allOf": [
							{
								"$ref": "#/components/schemas/URL"
							}
						],
						"nullable": true
					}
				},
				"required": [
					"name"
				],
				"type": "object",
				"additionalProperties": false
			}
		},
		"securitySchemes": {}
	},
	"info": {
		"title": "menuchi-node-backend",
		"version": "1.0.0",
		"description": "The NodeJS backend of Menuchi",
		"license": {
			"name": "ISC"
		},
		"contact": {}
	},
	"paths": {
		"/restaurants": {
			"post": {
				"operationId": "CreateRestaurant",
				"responses": {
					"201": {
						"description": "Restaurant, a branch and its backlog created successfully.",
						"content": {
							"application/json": {
								"schema": {
									"$ref": "#/components/schemas/RestaurantCompleteOut"
								}
							}
						}
					}
				},
				"tags": [
					"Restaurant"
				],
				"security": [],
				"parameters": [],
				"requestBody": {
					"required": true,
					"content": {
						"application/json": {
							"schema": {
								"$ref": "#/components/schemas/RestaurantCompactIn"
							}
						}
					}
				}
			}
		}
	},
	"servers": [
		{
			"url": "/"
		}
	]
}